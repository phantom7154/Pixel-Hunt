{
  "manifest_version": 3,
  "name": "Pixel-hunt",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "characters/cat1.png"
  },
  "permissions": ["scripting", "storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["characters/*.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
